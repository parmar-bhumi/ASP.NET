@model List<insert_display_data.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

<td>@Html.ActionLink("Add", "Registration")</td>

<table border="2" class="table  table-striped table-hover">
    <tr>
        <td>Id</td>
        <td>Fname</td>
        <td>Lname</td>
        <td>Edit</td>
        <td>Delete</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.fname</td>
            <td>@item.lname</td>
            <td>@Html.ActionLink("Edit", "Registration", new { id = item.Id }, new { @class = "btn btn-primary" })</td>
           <td>@Html.ActionLink("Delete", "delete", new { id = item.Id }, new{@class = "btn btn-danger" })</td>   
        </tr>
    }

</table>

